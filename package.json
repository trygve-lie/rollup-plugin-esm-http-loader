{
  "name": "rollup-plugin-esm-http-resolver",
  "version": "1.0.1",
  "description": "Rollup plugin for resolving ES modules over http(s)",
  "main": "dist/plugin.cjs.js",
  "directories": {
    "dist": "dist"
  },
  "scripts": {
    "test": "tap test/*.mjs",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "start": "node --experimental-modules ./example/server.mjs",
    "build": "rollup -c"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/trygve-lie/rollup-plugin-esm-http-resolver.git"
  },
  "keywords": [
    "rollup-plugin",
    "rollup.js",
    "resolve",
    "rollup",
    "esm"
  ],
  "author": "Trygve Lie",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/trygve-lie/rollup-plugin-esm-http-resolver/issues"
  },
  "homepage": "https://github.com/trygve-lie/rollup-plugin-esm-http-resolver#readme",
  "devDependencies": {
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-plugin-import": "2.18.2",
    "eslint": "6.3.0",
    "rollup": "1.17.0",
    "tap": "14.4.3"
  },
  "dependencies": {
    "node-fetch": "2.6.0"
  }
}
